# Define Service
service AZenith /vendor/bin/hw/vendor.azenith-service
    seclabel u:r:init:s0
    class late_start
    user root
    group root mediadrm media_rw sdcard_rw media system sdcard_r
    disabled

service AZenith_pre /vendor/bin/AZenith_config
    seclabel u:r:shell:s0
    class late_start
    user root
    group mediadrm media_rw sdcard_rw media system sdcard_r
    oneshot
    disabled

# Exec service
on property:sys.boot_completed=1
    start AZenith_pre
    start AZenith
